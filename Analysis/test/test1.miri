program salidas;

declare 
float fRespuesta;

funct float fSuma(float fX, float fY){
    fRespuesta = fX + fY;
    return fRespuesta;
}

funct float fResta(float fX, float fY){
    fRespuesta = fX - fY;
    return fRespuesta;
}

funct float fMult(float fX, float fY){
    fRespuesta = fX * fY;
    return fRespuesta;
}

funct float fDfv(float fX, float fY){
    fRespuesta = fX/fY;
    return fRespuesta;
}
main(){
    declare
    float fA, fB, fC;

    read(fA);
    read(fB);

    fC = fSuma(fA,fB);
    write(fC);

    fC = fResta(fA,fB);
    write(fC);

    fC = fMult(fA,fB);
    write(fC);

    fC = fDfv(fA,fB);
    write(fC);
    
}
end;
